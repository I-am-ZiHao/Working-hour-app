{
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": "react-app",
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-use-before-define": "warn",
    "react/prop-types": "off",
    "camelcase": "off",
    "no-unused-vars": "warn",
    "no-undef": "off"
  }
}
