{
  "expo": {
    "name": "Work Hour Record",
    "description": "An iOS App used to record working hours",
    "slug": "work-hour-record",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
